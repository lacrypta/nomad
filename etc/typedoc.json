{
  "$schema": "https://typedoc.org/schema.json",
  "includeVersion": true,
  "plugin": [
    "typedoc-plugin-expand-object-like-types",
    "typedoc-plugin-keywords",
    "typedoc-plugin-mdn-links",
    "typedoc-plugin-mermaid",
    "typedoc-plugin-remove-references"
  ],
  "keywords": ["NOSTR", "library", "Nomad"],
  "markedOptions": {
    "gfm": true
  },
  "excludeExternals": true,
  "pretty": true,
  "sourceLinkExternal": true,
  "hideParameterTypesInTitle": false,
  "excludePrivate": true,
  "cleanOutputDir": true,
  "hideGenerator": true,
  "searchInComments": true,
  "navigationLinks": {
    "La Crypta": "https://lacrypta.ar"
  },
  "visibilityFilters": {},
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": true
  },
  "treatWarningsAsErrors": false,
  "treatValidationWarningsAsErrors": false,
  "logLevel": "Verbose",
  "disableSources": false,
  "tsconfig": "./tsconfig.build.json",
  "customCss": "./typedoc-tweaks.css",
  "sort": ["static-first", "visibility", "kind", "alphabetical"],
  "kindSortOrder": [
    "Reference",
    "Project",
    "Module",
    "Namespace",
    "TypeAlias",
    "TypeParameter",
    "TypeLiteral",
    "Enum",
    "EnumMember",
    "Interface",
    "CallSignature",
    "IndexSignature",
    "Class",
    "Constructor",
    "ConstructorSignature",
    "Accessor",
    "GetSignature",
    "SetSignature",
    "Method",
    "Property",
    "Function",
    "Variable",
    "Parameter"
  ]
}
